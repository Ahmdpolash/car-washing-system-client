{
  // "include": ["src"],
  // "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "es2016",

    /* Modules */
    "module": "CommonJS",
    "rootDir": "./src" /* Specify the root folder within your source files. */,

    "outDir": "./dist",

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true /* Ensure that casing is correct in imports. */,

    "strict": true,

    "skipLibCheck": true
  }
}
